SELECT * FROM PRO_PAIS;


SELECT FC_BUSCA_CIDADE FROM DUAL


SELECT F.NOM_FUNC,
       FC_BUSCA_CIDADE(F.CDCIDADE)
FROM PRO_FUNC F

CREATE SEQUENCE PRO_PAIS_SEQ
INCREMENT BY 1 
MINVALUE 1
MAXVALUE 9999999999
START WITH 4
NOCACHE
NOCYCLE
